modules = ["python-3.11", "nodejs-20"]
[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "static-site"

[[workflows.workflow]]
name = "static-site"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx serve -l 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[deployment]
deploymentTarget = "static"
publicDir = "."

[nix]
channel = "stable-25_05"
packages = ["ffmpeg"]

[userenv]

[userenv.shared]

[objectStorage]
defaultBucketID = "replit-objstore-1c5ac02f-8a92-4ee0-97e5-870b4c350330"
